            function sj_be(el, evt, mth) {
                if (el.addEventListener) {
                    el.addEventListener(evt, mth, false);
                }
                else if (el.attachEvent) {
                    el.attachEvent("on" + evt, mth);
                }
                else {
                    el["on" + evt] = mth;
                }
